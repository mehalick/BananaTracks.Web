@model IReadOnlyCollection<BananaTracks.Entities.Workout>
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>BananaTracks</title>
</head>
<body>
<pre>
   ___                              _____                _        
  / __\ __ _ _ __   __ _ _ __   __ /__   \_ __ __ _  ___| | _____ 
 /__\/// _` | '_ \ / _` | '_ \ / _` |/ /\/ '__/ _` |/ __| |/ / __|
/ \/  \ (_| | | | | (_| | | | | (_| / /  | | | (_| | (__|   &lt;\__ \
\_____/\__,_|_| |_|\__,_|_| |_|\__,_\/   |_|  \__,_|\___|_|\_\___/                                                           
</pre>
    
<table>
    <thead>
    <tr>
        <th>Id</th>
        <th>Activity</th>
        <th>Start</th>
        <th>End</th>
    </tr>
    </thead>
    <tbody>
        @foreach (var workout in Model)
        {
            <tr>
                <td>@workout.Id</td>
                <td>@workout.Activity.Name</td>
                <td>@workout.StartUtc</td>
                <td>@workout.EndUtc</td>
            </tr>
        }
    </tbody>
</table>

</body>
</html>